<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.homemade.akhilez.timetable">

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <receiver
            android:name=".AlarmReceiver1"
            android:enabled="true" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".About"
            android:label="@string/title_activity_about"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ImportantNotes"
            android:label="@string/title_activity_important_notes"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ChangeTimeTable"
            android:label="@string/title_activity_change_time_table"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EditTimetable"
            android:label="@string/title_activity_edit_timetable"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".editingActivities.EditTimings"
            android:label="@string/title_activity_edit_timings"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".editingActivities.ChooseTimeTable"
            android:label="Premade TimeTables"
            android:theme="@style/AppTheme"/><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>